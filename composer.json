        }
    ],
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "react/http": "^1.1",
        "geoip2/geoip2": "~2.0",
        "react/filesystem": "^0.1.2",
        "react/child-process": "^0.6.1",
        "illuminate/console": "^8.10",
        "illuminate/container": "^8.10",
        "illuminate/events": "^8.10",
        "ext-json": "*",
        "nikic/fast-route": "^1.3",
        "illuminate/filesystem": "^8.10"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "GeoIPServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeoIPServer\\Tests\\": "tests/"
        }
    }
